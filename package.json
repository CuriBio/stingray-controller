{
  "name": "StingrayController",
  "version": "0.0.1",
  "description": "Desktop application for controlling the Stingray instrument.",
  "author": {
    "name": "Curi Bio",
    "email": "contact@curibio.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CuriBio/stingray-controller.git"
  },
  "homepage": "http://www.curibio.com",
  "private": false,
  "license": "MIT",
  "main": "./dist/main/index.js",
  "scripts": {
    "prepare": "pre-commit install",
    "dev": "check-engines && cross-env-shell NODE_ENV=development node .electron-nuxt/index.js",
    "test:unit": "check-engines && cross-env-shell NODE_ENV=test jest -c jest.config.js",
    "build-unstable": "check-engines && cross-env-shell NODE_ENV=production RELEASE_CHANNEL=unstable node .electron-nuxt/index.js",
    "build-prod": "check-engines && cross-env-shell NODE_ENV=production RELEASE_CHANNEL=prod node .electron-nuxt/index.js",
    "electron-builder-help": "electron-builder --help",
    "postinstall": "electron-builder install-app-deps",
    "org-imports": "organize-imports-cli src/main/index.dev.js"
  },
  "engines": {
    "node": "^16.17.0"
  },
  "resolutions": {
    "@types/node": "^16.17.0"
  }
}
